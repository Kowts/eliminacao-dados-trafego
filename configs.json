{
    "process": {
        "name": "Retenção de Dados Tráfego",
        "identifier": "ETL026",
        "description": "Purge diário que assegura que a tabela f_trafegoc01 conserva apenas registos de tráfego dos últimos 3 meses, eliminando dados mais antigos.",
        "version": "2.1.0",
        "execMode": "daily",
        "execDay": 2,
        "execTime": "06:00"
    },
    "database": {
        "webmail": {
            "table": "platforms",
            "where": {
                "clause": "identifier = %s",
                "params": "CVT_Webmail"
            }
        }
    },
    "report": {
        "from_mail": "dsi-suporte-rpa@cvt.cv",
        "to": "DMK-BI@cvt.cv",
        "cc": ["alertas.rpa@cvt.cv"],
        "subject": "Alerta: Retenção de Dados Tráfego"
    },
    "error_report": {
        "from_mail": "dsi-suporte-rpa@cvt.cv",
        "to": "alertas.rpa@cvt.cv",
        "subject": "Erro: Retenção de Dados Tráfego"
    }
}
